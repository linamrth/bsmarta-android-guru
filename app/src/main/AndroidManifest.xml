<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.linameritha.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/teacher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Fitur.SplashScreen.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Fitur.LoginForm.LoginForm"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Fitur.Menu.ButtomNavigation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Fitur.Siswa.DetailsiswaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Fitur.SemuaSiswa.DetailsemuasiswaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Fitur.Siswa.InputrapotActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Fitur.SemuaSiswa.ViewrapotActivity"
            android:theme="@style/AppTheme2" />
    </application>

</manifest>